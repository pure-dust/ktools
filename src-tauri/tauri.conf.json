{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ktools",
  "version": "0.1.0",
  "identifier": "com.ktools.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:52034/pages/main/index.html",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "resources": {
      "icons/icon.ico": "./icon.ico",
      "plugins/": "./plugins"
    },
    "windows": {
      "wix": {
        "language": "zh-CN"
      },
      "nsis": {
        "languages": [
          "SimpChinese"
        ]
      }
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
